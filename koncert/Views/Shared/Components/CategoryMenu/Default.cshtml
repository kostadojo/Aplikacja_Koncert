@model IEnumerable<Category>

<ul class="navbar-nav d-flex flex-row justify-content-center align-items-center" style="list-style: none; padding: 0; margin: 0;">
    @foreach (var category in Model)
    {
        <li class="nav-item" style="margin-right: 1rem;">
            <a asp-controller="Concert"
               asp-action="List"
               asp-route-category="@category.Name"
               class="nav-link" style="padding: 0.5rem 1rem;">
                @category.Name
            </a>
        </li>
    }
    <li class="nav-item" style="margin-right: 1rem;">
        <a asp-controller="Concert"
           asp-action="List"
           asp-route-Category=""
           class="nav-link" style="padding: 0.5rem 1rem;">
            WSZYSTKIE
        </a>
    </li>
</ul>